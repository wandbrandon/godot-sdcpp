[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../godot-sdcpp/target/debug/libgodot_sdcpp.so"
linux.release.x86_64 =   "res://../godot-sdcpp/target/release/libgodot_sdcpp.so"
windows.debug.x86_64 =   "res://../godot-sdcpp/target/debug/godot_sdcpp.dll"
windows.release.x86_64 = "res://../godot-sdcpp/target/release/godot_sdcpp.dll"
macos.debug =            "res://../godot-sdcpp/target/debug/libgodot_sdcpp.dylib"
macos.release =          "res://../godot-sdcpp/target/release/libgodot_sdcpp.dylib"
macos.debug.arm64 =      "res://../godot-sdcpp/target/debug/libgodot_sdcpp.dylib"
macos.release.arm64 =    "res://../godot-sdcpp/target/release/libgodot_sdcpp.dylib"